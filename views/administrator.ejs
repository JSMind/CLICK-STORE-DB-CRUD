              <th scope="col">NOMBRE PRODUCTO <%= producto.nombre_producto %> </th>
              <th scope="col">PRECIO <%= producto.precio%> </th>
              <!-- <th scope="col">IMAGEN</th> -->
              <th scope="col">DESCRIPCION <%= producto.descripcion %> </th>
              <th scope="col">STOCK <%= producto.stock%> </th>
              <th scope="col">ID_CATEGORIA <%= producto.id_categoria %> </th>
              <th scope="col">FECHA_REGISTRO <%= producto.Fecha_Registro %> </th>
              <th scope="col">FECHA_ACTUALIZACION <%= producto.Fecha_Actualizacion %> </th>
              <%  }) %>
            </tr>
              
          </thead>
          
